{
	"name": "blog-list",
	"version": "0.1.0",
	"private": true,
	"homepage": ".",
	"dependencies": {
		"@testing-library/jest-dom": "^5.11.4",
		"@testing-library/react": "^11.1.0",
		"@testing-library/user-event": "^12.1.10",
		"axios": "^0.21.1",
		"bcrypt": "^5.0.1",
		"concurrently": "^6.4.0",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"express-async-errors": "^3.1.1",
		"http": "^0.0.1-security",
		"jsonwebtoken": "^8.5.1",
		"lodash": "^4.17.21",
		"mongoose": "^5.11.13",
		"mongoose-unique-validator": "^2.0.3",
		"prop-types": "^15.7.2",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-scripts": "^5.0.0",
		"web-vitals": "^1.0.1"
	},
	"scripts": {
		"start": "concurrently --kill-others-on-fail \"npm run api:prod\" \"npm run frontend\"",
		"dev": "concurrently --kill-others-on-fail \"npm run api:dev\" \"npm run frontend\"",
		"build": "cd frontend && npm run build",
		"test": "NODE_ENV=test jest --verbose --runInBand --unhandled-rejections=strict",
		"lint": "eslint .",
		"frontend": "cd frontend && npm run start",
		"frontend:test-env": "concurrently --kill-others-on-fail \"npm run api:start:test\" \"npm run frontend\"",
		"postinstall": "cd frontend && npm install",
		"frontend:test": "cd frontend && npm run test",
		"api:prod": "NODE_ENV=production node index.js",
		"api:start:test": "NODE_ENV=test node index.js",
		"api:dev": "NODE_ENV=development nodemon index.js",
		"eject": "react-scripts eject",
		"heroku-postbuild": "cd frontend && npm install && npm run build"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^12.1.2",
		"cypress": "^8.7.0",
		"eslint": "^7.19.0",
		"eslint-config-google": "^0.14.0",
		"eslint-plugin-cypress": "^2.12.1",
		"eslint-plugin-react": "^7.26.1",
		"jest": "^26.6.3",
		"nodemon": "^2.0.13",
		"supertest": "^6.1.6"
	},
	"jest": {
		"testEnvironment": "node"
	}
}
