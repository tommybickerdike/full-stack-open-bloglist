{
	"name": "blog-list",
	"version": "0.1.0",
	"private": true,
	"main": "index.js",
	"engines": {
		"node": "15.x"
	},
	"dependencies": {
		"@testing-library/jest-dom": "^5.11.4",
		"@testing-library/react": "^11.1.0",
		"@testing-library/user-event": "^12.1.10",
		"axios": "^0.21.1",
		"bcrypt": "^5.0.1",
		"concurrently": "^6.4.0",
		"cors": "^2.8.5",
		"cross-env": "^7.0.3",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"express-async-errors": "^3.1.1",
		"http": "^0.0.1-security",
		"jsonwebtoken": "^8.5.1",
		"lodash": "^4.17.21",
		"mongoose": "^5.11.13",
		"mongoose-unique-validator": "^2.0.3",
		"prop-types": "^15.7.2",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-scripts": "^5.0.0",
		"web-vitals": "^1.0.1",
		"@emotion/react": "^11.6.0",
		"@emotion/styled": "^11.6.0",
		"@mui/icons-material": "^5.1.0",
		"@mui/material": "^5.1.0",
		"react-redux": "^7.2.6",
		"react-router": "^5.2.0",
		"react-router-dom": "^5.2.0",
		"redux": "^4.1.2",
		"redux-thunk": "^2.4.0",
		"styled-components": "^5.3.3"
	},
	"scripts": {
		"start": "cd frontend && npm run start",
		"dev": "concurrently --kill-others-on-fail \"npm run api:dev\" \"npm run frontend\"",
		"build": "cd frontend && npm run build",
		"test:env": "concurrently --kill-others-on-fail \"cross-env NODE_ENV=test node index.js\" \"npm run frontend\"",
		"test": "cross-env NODE_ENV=test jest --verbose --runInBand",
		"lint": "eslint .",
		"postinstall": "cd frontend && npm install",
		"frontend:test": "cypress run",
		"api:prod": "cross-env NODE_ENV=production node index.js",
		"api:dev": "cross-env NODE_ENV=development nodemon index.js",
		"eject": "react-scripts eject"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^12.1.2",
		"cypress": "^9.1.1",
		"eslint": "^7.19.0",
		"eslint-config-google": "^0.14.0",
		"eslint-plugin-cypress": "^2.12.1",
		"eslint-plugin-react": "^7.26.1",
		"jest": "^26.6.3",
		"nodemon": "^2.0.13",
		"supertest": "^6.1.6"
	},
	"jest": {
		"testEnvironment": "node"
	}
}
